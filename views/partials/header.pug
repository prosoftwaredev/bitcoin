.header.header-filter(style="background-image: url('https://images.unsplash.com/photo-1423655156442-ccc11daa4e99?crop=entropy&dpr=2&fit=crop&fm=jpg&h=750&ixjsv=2.1.0&ixlib=rb-0.3.5&q=50&w=1450');")
  .card.card-raised.card-carousel
    #carousel-example-generic.carousel.slide(data-ride='carousel')
      .carousel.slide(data-ride='carousel')
        // Indicators
        ol.carousel-indicators
          li.active(data-target='#carousel-example-generic', data-slide-to='0')
          li(data-target='#carousel-example-generic', data-slide-to='1')
          li(data-target='#carousel-example-generic', data-slide-to='2')
        // Wrapper for slides
        .carousel-inner
          .item.active
            img(src='/images/bg2.jpeg', alt='Awesome Image')
            .carousel-caption
              h4
                i.material-icons location_on
                |  Yellowstone National Park, United States
          .item
            img(src='/images/bg3.jpeg', alt='Awesome Image')
            .carousel-caption
              h4
                i.material-icons location_on
                |  Somewhere Beyond, United States
          .item
            img(src='/images/bg4.jpeg', alt='Awesome Image')
            .carousel-caption
              h4
                i.material-icons location_on
                |  Yellowstone National Park, United States
        // Controls
        a.left.carousel-control(href='#carousel-example-generic', data-slide='prev')
          i.material-icons keyboard_arrow_left
        a.right.carousel-control(href='#carousel-example-generic', data-slide='next')
          i.material-icons keyboard_arrow_right